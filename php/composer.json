{
    "name": "github/copilot-sdk",
    "description": "PHP SDK for the GitHub Copilot CLI - communicate with Copilot via JSON-RPC 2.0 over stdio or TCP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "github",
        "copilot",
        "sdk",
        "json-rpc",
        "ai",
        "llm"
    ],
    "authors": [
        {
            "name": "Microsoft Corporation"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "GitHub\\Copilot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GitHub\\Copilot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
